<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="line">
        <stroke android:width="2dp" android:color="@color/blue_i"/>
        </shape>
    </item>

    <item>
        <rotate
            android:fromDegrees="90">
            <shape android:shape="line">
                <stroke android:width="2dp" android:color="@color/blue_i" />
            </shape>
        </rotate>
    </item>
</layer-list>